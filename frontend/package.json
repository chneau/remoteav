{
  "name": "remoteav",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check": "npm exec tsc -- --noEmit --skipLibCheck",
    "format": "npm exec prettier -- --write . --ignore-path .gitignore",
    "update": "npm exec --yes npm-check-updates@latest -- --upgrade --target=minor && npm update",
    "update-major": "npm exec --yes npm-check-updates@latest -- --upgrade && npm update"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^1.3.2",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vite": "^2.9.14"
  },
  "prettier": {
    "printWidth": 150
  }
}
