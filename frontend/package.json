{
  "name": "remoteav",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check": "npm exec tsc -- --noEmit --skipLibCheck",
    "format": "npm exec prettier -- --write . --ignore-path .gitignore",
    "update": "npm exec --yes npm-check-updates@latest -- --upgrade --target=minor && npm update",
    "update-major": "npm exec --yes npm-check-updates@latest -- --upgrade && npm update",
    "graphql-codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "graphql": "^16.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.9.1",
    "@graphql-codegen/typescript": "2.7.2",
    "@graphql-codegen/typescript-operations": "2.5.2",
    "@graphql-codegen/typescript-react-apollo": "3.3.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.2"
  },
  "prettier": {
    "printWidth": 150
  }
}
